<?php
use Core\Console;

require_once(__DIR__ . "/vendor/autoload.php");

$command = $argv[1];

switch ($command) {
    case 'help':
        Console::help();
        break;
    case 'view-fresh':
        Console::view_fresh();
        break;
    case 'delete-logs':
        Console::delete_logs();
        break;
    default:
        echo("command: $command not exist use help to show the command list");
        break;
}